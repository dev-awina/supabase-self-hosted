# Configuration to be found here: https://github.com/supabase/auth?tab=readme-ov-file#configuration

GOTRUE_SMTP_ADMIN_EMAIL=admin@example.com
GOTRUE_SMTP_HOST=supabase-mail
GOTRUE_SMTP_PORT=2500
GOTRUE_SMTP_USER=fake_mail_user
GOTRUE_SMTP_PASS=fake_mail_password
GOTRUE_SMTP_SENDER_NAME=fake_sender

# Define custom email templates like this
GOTRUE_MAILER_TEMPLATES_CONFIRMATION=https://path-to-your/confirm-email.html
GOTRUE_MAILER_TEMPLATES_RECOVERY=https://path-to-your/reset-password.html

GOTRUE_EXTERNAL_GOOGLE_ENABLED=true
GOTRUE_EXTERNAL_GOOGLE_CLIENT_ID='YOUR VALUE'
GOTRUE_EXTERNAL_GOOGLE_SECRET='YOUR VALUE'
GOTRUE_EXTERNAL_GOOGLE_REDIRECT_URI=http://localhost:8000/auth/v1/callback
GOTRUE_EXTERNAL_GOOGLE_SKIP_NONCE_CHECK=true

GOTRUE_EXTERNAL_FACEBOOK_ENABLED=true
GOTRUE_EXTERNAL_FACEBOOK_CLIENT_ID='YOUR VALUE'
GOTRUE_EXTERNAL_FACEBOOK_SECRET='YOUR VALUE'
GOTRUE_EXTERNAL_FACEBOOK_REDIRECT_URI=http://localhost:8000/auth/v1/callback
GOTRUE_EXTERNAL_FACEBOOK_SKIP_NONCE_CHECK=true


 